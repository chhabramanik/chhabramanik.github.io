    <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>
        <script>
  var $form = $('form#test-form'),
      url = 'https://script.google.com/macros/s/AKfycbx5gT4C57w09xyzM95bGC_2nZVa-HM2e_w8Bwm4YFJ5Bt-bPIsz/exec'

  $('#submit-form').on('click', function(e) {

    e.preventDefault();
    var x = document.forms["myForm"]["first_name"].value;
    if ( x == null || x == "" ) {
        alert('Please Fill First Name')
        myForm.first_name.focus();
        return false;
    }
    var y = document.forms["myForm"]["last_name"].value;
    if ( y == null || y == "" ) {
        alert('Please Fill Last Name')
        myForm.last_name.focus();
        return false;
    }
    var z = document.forms["myForm"]["dob"].value;
    if ( z == null || z == "" ) {
        alert('Please Fill DOB')
        myForm.dob.focus();
        return false;
    }
    var a = document.forms["myForm"]["email"].value;
    if ( a == null || a == "" ) {
        alert('Please Fill Email')
        myForm.email.focus();
        return false;
    }
    var b = document.forms["myForm"]["phone"].value;
    if ( b == null || b == "" ) {
        alert('Please Fill Phone Number')
        myForm.phone.focus();
        return false;
    }
    var c = document.forms["myForm"]["whatsapp"].value;
    if ( c == null || c == "" ) {
        alert('Please Fill Whatsapp Number')
        myForm.whatsapp.focus();
        return false;
    }
    var d = document.forms["myForm"]["firstpreference"].value;
    if ( d == null || d == "" ) {
        alert('Please Fill First Preference')
        myForm.firstpreference.focus();
        return false;
    }
    var e = document.forms["myForm"]["secondpreference"].value;
    if ( e == null || e == "" ) {
        alert('Please Fill Second Preference')
        myForm.secondpreference.focus();
        return false;
    }
    var f = document.forms["myForm"]["thirdpreference"].value;
    if ( f == null || f == "" ) {
        alert('Please Fill Third Preference')
        myForm.thirdpreference.focus();
        return false;
    }
    var g = document.forms["myForm"]["College"].value;
    if ( g == null || g == "" ) {
        alert('Please Fill Third Preference')
        myForm.College.focus();
        return false;
    }
    else
    var jqxhr = $.ajax({
      url: url,
      method: "GET",
      dataType: "json",
      data: $form.serialize()

    }).success(
      validateForm()
    );
  })
  </script>

  <script>
  function validateForm() {
   var x = document.forms["myForm"]["phone"].value;
   if ( x == null || x == "" ) {
   }

